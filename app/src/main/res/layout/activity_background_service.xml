<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#010101"
    android:backgroundTint="#010101"
    tools:context=".location"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="12dp">

    <TextView
        android:id="@+id/longitude_service"
        android:layout_width="204dp"
        android:layout_height="27dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="108dp"
        android:background="#FFFFFF"
        android:backgroundTint="#000000"
        android:text="Получение данных..."
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/latitude_text2"
        app:layout_constraintTop_toBottomOf="@+id/bttn_gsm" />

    <TextView
        android:id="@+id/latitude_service"
        android:layout_width="204dp"
        android:layout_height="27dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="76dp"
        android:background="#FFFFFF"
        android:backgroundTint="#000000"
        android:text="Получение данных..."
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/latitude_text2"
        app:layout_constraintTop_toBottomOf="@+id/bttn_gsm" />

    <TextView
        android:id="@+id/altitude_service"
        android:layout_width="204dp"
        android:layout_height="27dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="140dp"
        android:background="#FFFFFF"
        android:backgroundTint="#000000"
        android:text="Получение данных..."
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/latitude_text2"
        app:layout_constraintTop_toBottomOf="@+id/bttn_gsm" />

    <TextView
        android:id="@+id/accuracy_service"
        android:layout_width="204dp"
        android:layout_height="27dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="204dp"
        android:background="#FFFFFF"
        android:backgroundTint="#000000"
        android:text="Получение данных..."
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/latitude_text2"
        app:layout_constraintTop_toBottomOf="@+id/bttn_gsm" />

    <TextView
        android:id="@+id/current_time_service"
        android:layout_width="204dp"
        android:layout_height="27dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="172dp"
        android:background="#FFFFFF"
        android:backgroundTint="#000000"
        android:text="Получение данных..."
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/latitude_text2"
        app:layout_constraintTop_toBottomOf="@+id/bttn_gsm" />

    <TextView
        android:id="@+id/latitude_text2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginTop="76dp"
        android:text="Широта:"
        android:textColor="#6A6A6A"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bttn_lte" />

    <TextView
        android:id="@+id/latitude_text4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginTop="140dp"
        android:text="Высота:"
        android:textColor="#6A6A6A"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bttn_lte" />

    <TextView
        android:id="@+id/latitude_text5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginTop="172dp"
        android:text="Время:"
        android:textColor="#6A6A6A"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bttn_lte" />

    <TextView
        android:id="@+id/dgf"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginTop="204dp"
        android:text="Точность:"
        android:textColor="#6A6A6A"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bttn_lte" />

    <TextView
        android:id="@+id/latitude_text3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginTop="108dp"
        android:text="Долгота:"
        android:textColor="#6A6A6A"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bttn_lte" />

    <TextView
        android:id="@+id/CellSignalStrength"
        android:layout_width="388dp"
        android:layout_height="45dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="12dp"
        android:textColor="#FFFFFF"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/list_cell_info1" />

    <TextView
        android:id="@+id/cellIdentity"
        android:layout_width="388dp"
        android:layout_height="52dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="16dp"
        android:textColor="#FFFFFF"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/CellIdentity"
        android:layout_width="388dp"
        android:layout_height="45dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="16dp"
        android:textColor="#FFFFFF"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/bttn_nr"
        android:layout_width="116dp"
        android:layout_height="68dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="456dp"
        android:backgroundTint="#16c603"
        android:text="NR"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/bttn_gsm"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/sdghvj"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginTop="20dp"
        android:text="Трафик:"
        android:textColor="#6A6A6A"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bttn_lte" />

    <TextView
        android:id="@+id/traffic_total"
        android:layout_width="204dp"
        android:layout_height="27dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="20dp"
        android:background="#FFFFFF"
        android:backgroundTint="#000000"
        android:text="Получение данных..."
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/latitude_text2"
        app:layout_constraintTop_toBottomOf="@+id/bttn_gsm" />

    <Button
        android:id="@+id/bttn_gsm"
        android:layout_width="131dp"
        android:layout_height="68dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="456dp"
        android:backgroundTint="#16c603"
        android:text="GSM"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/bttn_lte"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/bttn_lte"
        android:layout_width="116dp"
        android:layout_height="68dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="456dp"
        android:backgroundTint="#16c603"
        android:text="LTE"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ListView
        android:id="@+id/list_cell_info1"
        android:layout_width="383dp"
        android:layout_height="120dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="4dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/CellIdentity" />

    <ListView
        android:id="@+id/list_cell_info2"
        android:layout_width="387dp"
        android:layout_height="120dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="8dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/CellSignalStrength" />

    <ListView
        android:id="@+id/top_apps_list"
        android:layout_width="384dp"
        android:layout_height="90dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/list_cell_info2"
        tools:layout_editor_absoluteX="8dp" />

</androidx.constraintlayout.widget.ConstraintLayout>